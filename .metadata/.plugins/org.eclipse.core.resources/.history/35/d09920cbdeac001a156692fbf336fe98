
import java.util.*;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

		Scanner sc = new Scanner(System.in);
		
		int N = 0;
	
 N = sc.nextInt();
 
 int i = 0;

 double a,b,c,x;
 
	for(i=0;i<N;i++) {
		a  = sc.nextDouble();
		b  = sc.nextDouble();
		c  = sc.nextDouble();
		x = (a+b+c)/N;
		System.out.printf("%.1f",x);
}
 		
		sc.close();

} }
