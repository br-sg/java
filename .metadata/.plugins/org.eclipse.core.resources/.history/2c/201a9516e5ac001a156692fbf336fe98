
import java.util.*;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

		Scanner sc = new Scanner(System.in);
		
int N = sc.nextInt();
int i=0;

for (i=1;i<=N;i++) {
	
	System.out.print(i + " " + (int) Math.pow(i, 2) + " " + (int) Math.pow(i, 2));
	System.out.printf("\n");
}

		
		sc.close();

} }
