
import java.util.*;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

		Scanner sc = new Scanner(System.in);
		
int N = sc.nextInt();
int x,y,i;

for (i=0;i<N;i++) {
x = sc.nextInt();
y = sc.nextInt();

if (y==0 ) {System.out.println("Divisão impossível");}
else {System.out.println(x/y);}
	
}

 		
		sc.close();

} }
