
import java.util.*;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

		Scanner sc = new Scanner(System.in);
		
int N = sc.nextInt();
int i, f;
f=0;

for (i=1;i<=N;i++) {
	
f = N * (N-i);

}

if (N==0) {f=1;}

System.out.println(f);
		
		sc.close();

} }
