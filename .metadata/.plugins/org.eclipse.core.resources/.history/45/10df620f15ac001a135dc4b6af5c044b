import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

double pi = 3.14159;
double area, raio;

Scanner sc = new Scanner(System.in);

System.out.println("Raio:");
raio = sc.nextDouble();
area = Math.pow(raio,2.0)*pi;
System.out.println("O valor da area é "+area);

sc.close();
	}
} 


