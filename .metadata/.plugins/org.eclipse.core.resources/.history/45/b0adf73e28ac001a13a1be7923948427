
import java.util.*;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

int a,b,c,d,e, z,x;
a = 1;
b = 2;
c = 3;
d = 4;
e = 5;
double f,g,h,i,j, check;
f = 4;
g = 4.50;
h = 5.00;
i = 2.00;
j = 1.50;
check = 1;

Scanner sc = new Scanner (System.in);
z = sc.nextInt();
x = sc.nextInt();

if (z == a || x == a) {
	check = f + check;
}
else if (z == b || x ==b) {
	check = g + check;
}
else if (z == c || x ==c) {
	check = h + check;}
else if (z == d || x == d) {
	check = i + check;
}
else if (z == e || x == e) {
check = j + check;}

System.out.println("Total: R$ "+ check);

sc.close(); }
}



