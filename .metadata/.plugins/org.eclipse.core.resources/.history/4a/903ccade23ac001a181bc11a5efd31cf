
import java.util.*;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);

	int num;
		
Scanner sc = new Scanner (System.in);

num = sc.nextInt();

if (num>=0) {
	System.out.println("NAO NEGATIVO");
	}
else System.out.println("NEGATIVO");

sc.close();


		
	}
} 


